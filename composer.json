{
    "name": "notix/gcaptcha",
    "description": "NOTIX - Google ReCaptcha for laravel",
    "keywords": [
        "recaptcha",
        "laravel",
        "google",
        "google-recaptcha",
        "laravel-recaptcha"
    ],
    "autoload": {
        "psr-4": {
            "Notix\\GCaptcha\\": "src"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ourmine-Notix",
            "email": "developer@notix.pro",
            "homepage": "https://notix.pro"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.*|6.*|7.*|8.*|9.*",
        "google/recaptcha": "1.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Notix\\GCaptcha\\CaptchaServiceProvider"
            ],
            "aliases": {
                "Captcha": "Notix\\GCaptcha\\CaptchaFacade"
            }
        }
    }
}
